{
  "permissions": {
    "allow": [
      "mcp__sequential-thinking__sequentialthinking"
    ],
    "deny": [],
    "ask": []
  },
  "hooks": {
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "python3 /Users/flink/parallel-dev-mcp/examples/hooks/web_message_sender.py"
          }
        ]
      }
    ],
    "Notification": [
      {
        "matcher": "*",
